recurrence:
  interval: "1h"
  batch_size: 100
  min_elapsed: "1h"

database:
  dbhost: "postgres"
  dbport: 5432
  dbuser: postgres
  dbpass: postgres
  dbname: minder
  sslmode: disable

logging:
  level: "debug"

events:
  sql_connection:
    dbhost: "watermill-postgres"
    dbport: 5432
    dbuser: postgres
    dbpass: postgres
    dbname: watermill
    sslmode: disable
